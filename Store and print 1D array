#include <stdio.h>

int main() {
    int arr[5];  // Declare a 1D array of size 5
    int i;

    // Input elements from user
    printf("Enter 5 elements:\n");
    for (i = 0; i < 5; i++) {
        printf("Element %d: ", i + 1);
        scanf("%d", &arr[i]);
    }

    // Print the array elements
    printf("\nYou entered:\n");
    for (i = 0; i < 5; i++) {
        printf("Element %d = %d\n", i + 1, arr[i]);
    }

    return 0;
}
