#include <stdio.h>

int main() {
    int n = 15;
    int first = 0, second = 1, next;

    printf("First %d Fibonacci numbers:\n", n);

    for (int i = 1; i <= n; i++) {
        printf("%d ", first);
        next = first + second;
        first = second;
        second = next;
    }

    return 0;
}
